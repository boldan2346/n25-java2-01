package br.com.senaisp.bauru.secao02.aula03;

public class ArtAscii02 {

	public static void main(String[] args) {
		System.out.println("\r\n"
				+ "                           ______                         \r\n"
				+ "                     _.-*'\"      \"`*-._                   \r\n"
				+ "                _.-*'                  `*-._              \r\n"
				+ "             .-'                            `-.           \r\n"
				+ "  /`-.    .-'                  _.              `-.        \r\n"
				+ " :    `..'                  .-'_ .                `.      \r\n"
				+ " |    .'                 .-'_.' \\ .                 \\     \r\n"
				+ " |   /                 .' .*     ;               .-'\"     \r\n"
				+ " :   L                    `.     | ;          .-'         \r\n"
				+ "  \\.' `*.          .-*\"*-.  `.   ; |        .'            \r\n"
				+ "  /      \\        '       `.  `-'  ;      .'              \r\n"
				+ " : .'\"`.  .       .-*'`*-.  \\     .      (_               \r\n"
				+ " |              .'        \\  .             `*-.           \r\n"
				+ " |.     .      /           ;                   `-.        \r\n"
				+ " :    db      '       d$b  |                      `-.     \r\n"
				+ " .   :PT;.   '       :P\"T; :                         `.   \r\n"
				+ " :   :bd;   '        :b_d; :                           \\  \r\n"
				+ " |   :$$; `'         :$$$; |                            \\ \r\n"
				+ " |    TP              T$P  '                             ;\r\n"
				+ " :                        /.-*'\"`.                       |\r\n"
				+ ".sdP^T$bs.               /'       \\                       \r\n"
				+ "$$$._.$$$$b.--._      _.'   .--.   ;                      \r\n"
				+ "`*$$$$$$P*'     `*--*'     '  / \\  :                      \r\n"
				+ "   \\                        .'   ; ;   [bug]              \r\n"
				+ "    `.                  _.-'    ' /                       \r\n"
				+ "      `*-.                      .'                        \r\n"
				+ "          `*-._            _.-*'                          \r\n"
				+ "               `*=--..--=*'");
	}

}
